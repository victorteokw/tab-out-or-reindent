{
	"name": "tab-out-or-reindent",
	"displayName": "Tab Out or Reindent",
	"description": "Tab jump out pair or reindent selection.",
	"publisher": "yeannylam",
	"version": "0.1.0",
	"repository": {
		"type": "git",
		"url": "https://github.com/zhangkaiyulw/tab-out-or-reindent.git"
	},
	"engines": {
		"vscode": "^1.34.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:tabOutOrReindent"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "tabOutOrReindent",
				"title": "Tab Out or Reindent"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"postinstall": "node ./node_modules/vscode/bin/install",
		"test": "npm run compile && node ./node_modules/vscode/bin/test"
	},
	"devDependencies": {
		"typescript": "^3.3.1",
		"vscode": "^1.1.28",
		"tslint": "^5.12.1",
		"@types/node": "^10.12.21",
		"@types/mocha": "^2.2.42"
	}
}
